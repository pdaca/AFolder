<?xml version="1.0" encoding="UTF-8"?>

<!-- c1=sum, c2=c -->
<function name="loop" counters="2" symbolic="1" modes="2">


  <case mode="1">
    <symbol_constraint relation="GEQ" numeric="0"/>
    <ctr_update counter="1" element="1"/>
  </case>

  <case mode="1">
    <symbol_constraint relation="LT" numeric="0"/>
    <symbol_constraint relation="EQ" symbolic="1"/>
    <ctr_update counter="2" numeric="1"/>
  </case>

  <case mode="1">
    <symbol_constraint relation="LT" numeric="0"/>
    <symbol_constraint relation="NEQ" symbolic="1"/>
  </case>

</function>


<!-- computes length -->
<function name="len" counters="1" symbolic="0">

  <case>
    <ctr_update counter="1" numeric="1"/>
  </case>
  
</function>

